RewriteEngine on

RewriteRule ^playlist$ playlist.php [L]
RewriteRule ^status$ status.php [L]
RewriteRule ^remove$ remove.php [L]
RewriteRule ^search$ search.php [L]
RewriteRule ^queue$ queue.php [L]
RewriteRule ^control$ control.php [L]
RewriteRule ^cover/current/.*$ cover.php?artist=current [L]
RewriteRule ^cover/(.*)/(.*)$ cover.php?artist=$1&album=$2 [L]
RewriteRule ^disabled$ disabled.php [L]
RewriteRule ^disabled/status$ disabled.php?json=1 [L]
